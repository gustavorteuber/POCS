{
  "name": "typeorm-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "ts-node-dev --respawn --ignore-watch node_modules --no-notify --transpile-only src/index.ts",
    "migrate:create": "typeorm migration:create \"./typeorm/migrations/",
    "migrate:up": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:run -d ormconfig.ts",
    "migrate:down": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:revert -d ormconfig.ts",
    "start": "npm run migrate:up && npm run server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
